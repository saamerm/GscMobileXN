<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:local="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:layout_marginBottom="10dp">
    <TableLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">
        <TableRow
            android:weightSum="1"
            android:layout_marginBottom="10dp">
            <gsc.NunitoTextView
                android:text="@string/eligibilityResultsDependentParticipantLabel"
                android:textColor="@color/black"
                android:textSize="18dp"
                android:textStyle="bold"
                android:layout_weight="0.5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="0dp"
                android:gravity="left" />
            <gsc.NunitoTextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textColor="@drawable/list_button_text_color"
                android:textColorHighlight="@color/white"
                android:textSize="18sp"
                android:layout_weight="0.5"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="20dp"
                android:layout_centerVertical="true"
                android:gravity="left"
                local:MvxBind="Text Format('{0}: {1}',ParticipantNumber, ParticipantFullName);" />
        </TableRow>
        <TableRow
            android:weightSum="1"
            android:layout_marginBottom="10dp">
            <gsc.NunitoTextView
                android:text="@string/eligibilityResultsRecallExamLabel"
                android:textColor="@color/black"
                android:textSize="18dp"
                android:textStyle="bold"
                android:layout_weight="0.5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:gravity="left" />
            <gsc.NunitoTextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textColor="@drawable/list_button_text_color"
                android:textColorHighlight="@color/white"
                android:textSize="18sp"
                android:layout_weight="0.5"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="20dp"
                android:layout_centerVertical="true"
                android:gravity="left"
                local:MvxBind="Text EligibilityNullToMsg(RawRecallExamEligibilityDate,RecallExamEligibilityStatus);" />
        </TableRow>
        <TableRow
            android:weightSum="1"
            android:layout_marginBottom="10dp">
            <gsc.NunitoTextView
                android:text="@string/eligibilityResultsScalingLabel"
                android:textColor="@color/black"
                android:textSize="18dp"
                android:textStyle="bold"
                android:layout_weight="0.5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:gravity="left" />
            <gsc.NunitoTextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textColor="@drawable/list_button_text_color"
                android:textColorHighlight="@color/white"
                android:textSize="18sp"
                android:layout_weight="0.5"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="20dp"
                android:layout_centerVertical="true"
                android:gravity="left"
                local:MvxBind="Text EligibilityNullToMsg(RawScalingEligibilityDate,ScalingEligibilityStatus);" />
        </TableRow>
        <TableRow
            android:weightSum="1"
            android:layout_marginBottom="10dp">
            <gsc.NunitoTextView
                android:text="@string/eligibilityResultsPolishingLabel"
                android:textColor="@color/black"
                android:textSize="18dp"
                android:textStyle="bold"
                android:layout_weight="0.5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:gravity="left" />
            <gsc.NunitoTextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textColor="@drawable/list_button_text_color"
                android:textColorHighlight="@color/white"
                android:textSize="18sp"
                android:layout_weight="0.5"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="20dp"
                android:layout_centerVertical="true"
                android:gravity="left"
                local:MvxBind="Text EligibilityNullToMsg(RawPolishingEligibilityDate,PolishingEligibilityStatus);" />
        </TableRow>
    </TableLayout>
    <gsc.NunitoTextView
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@drawable/dotted_line"
        android:textSize="18sp"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_centerVertical="true" />
</LinearLayout>
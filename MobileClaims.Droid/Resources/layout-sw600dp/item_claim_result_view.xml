<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:local="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="3dp"
    android:layout_marginLeft="0dp"
    android:layout_marginRight="0dp">
    <TableLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <TableRow
            android:layout_marginTop="2dp"
            android:layout_marginBottom="5dp">
            <gsc.NunitoTextView
                android:text="@string/claimResultClaimDetailsLabel"
                android:textColor="@color/black"
                android:textSize="13sp"
                android:layout_marginLeft="20dp"
                android:layout_weight="0.6"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textStyle="bold"/>
            </TableRow>

        <TableRow
            android:layout_marginTop="2dp"
            android:layout_marginBottom="5dp">
            <gsc.NunitoTextView
                android:text="@string/claimResultFormNumberLabelPhone"
                android:textColor="@color/black"
                android:textSize="13sp"
                android:layout_marginLeft="20dp"
                android:layout_weight="0.6"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:gravity="left" />
            <gsc.NunitoTextView
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="13sp"
                android:layout_weight="0.4"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="20dp"
                android:layout_centerVertical="true"
                android:layout_gravity="left"
                android:gravity="left"
                local:MvxBind="Text ClaimFormID;" />
        </TableRow>
        <TableRow
            android:layout_marginTop="2dp"
            android:layout_marginBottom="5dp">
            <gsc.NunitoTextView
                android:text="@string/claimResultServiceDateLabelPhone"
                android:textColor="@color/black"
                android:textSize="13sp"
                android:layout_marginLeft="20dp"
                android:layout_weight="0.6"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:gravity="left"
                local:MvxBind="Visibility InvertedVisibility(HasHCSADetails);" />
            <gsc.NunitoTextView
                android:text="@string/claimResultExpenseDateLabelPhone"
                android:textColor="@color/black"
                android:textSize="13sp"
                android:layout_marginLeft="20dp"
                android:layout_weight="0.6"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:gravity="left"
                local:MvxBind="Visibility Visibility(HasHCSADetails);" />
            <gsc.NunitoTextView
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="13sp"
                android:layout_weight="0.4"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="20dp"
                android:layout_centerVertical="true"
                android:layout_gravity="left"
                android:gravity="left"
                local:MvxBind="Text Format('{0:MMMM dd, yyy}',ServiceDate);" />
        </TableRow>
        <TableRow
            android:layout_marginTop="2dp"
            android:layout_marginBottom="5dp">
            <gsc.NunitoTextView
                android:text="@string/claimResultServiceDescriptionLabelPhone"

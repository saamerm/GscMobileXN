<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:local="http://schemas.android.com/apk/res-auto"
    android:duplicateParentState="true"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
	android:background="@color/white">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="20dp">
        <gsc.HeaderTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="30dp"
            android:gravity="center"
            local:MvxBind="Text PaymentInformation"
            android:layout_gravity="center_horizontal"
            android:textAllCaps="true" />
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp">
            <gsc.NunitoTextView
                android:textColor="@color/claim_text"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text GreenShieldIDNumber"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
            <gsc.NunitoTextViewBold
                android:id="@+id/qn1"
                android:textColor="@color/claim_text_grey"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text Payment.PlanMemberDisplayID"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
        <!-- android:text="1234567890" -->
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp">
            <gsc.NunitoTextView
                android:id="@+id/qn1"
                android:textColor="@color/claim_text"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text PaymentMethod"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
            <gsc.NunitoTextViewBold
                android:id="@+id/qn1"
                android:textColor="@color/claim_text_grey"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text Payment.PaymentMethod"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp">
            <gsc.NunitoTextView
                android:id="@+id/qn1"
                android:textColor="@color/claim_text"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text PaymentAmount"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
            <gsc.NunitoTextViewBold
                android:id="@+id/qn1"
                android:textColor="@color/claim_text_grey"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text Format('${0}',Payment.PaymentAmountString)"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp">
            <gsc.NunitoTextView
                android:id="@+id/qn1"
                android:textColor="@color/claim_text"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text PaymentCurrency"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
            <gsc.NunitoTextViewBold
                android:id="@+id/qn1"
                android:textColor="@color/claim_text_grey"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text Payment.Currency"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            local:MvxBind="Visible  Payment.IsDepositNumberVisible"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp">
            <gsc.NunitoTextView
                android:id="@+id/qn1"
                android:textColor="@color/claim_text"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text DirectDepositNumber"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
            <gsc.NunitoTextViewBold
                android:id="@+id/qn1"
                android:textColor="@color/claim_text_grey"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text Payment.DepositNumber"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            local:MvxBind="Visible Payment.IsChequeNumberVisible"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp">
            <gsc.NunitoTextView
                android:id="@+id/qn1"
                android:textColor="@color/claim_text"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text ChequeNumberLabel"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
            <gsc.NunitoTextViewBold
                android:id="@+id/qn1"
                android:textColor="@color/claim_text_grey"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text Payment.ChequeNumber"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            local:MvxBind="Visible Payment.IsStatementDateVisible"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp">
            <gsc.NunitoTextView
                android:id="@+id/qn1"
                android:textColor="@color/claim_text"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text StatementDate"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
            <gsc.NunitoTextViewBold
                android:id="@+id/qn1"
                android:textColor="@color/claim_text_grey"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text Format('{0:MMM dd, yyy}',Payment.StatementDate)"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            local:MvxBind="Visible Payment.IsDepositDateVisible"
            android:layout_marginBottom="8dp">
            <gsc.NunitoTextView
                android:textColor="@color/claim_text"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text DepositDate"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
            <gsc.NunitoTextViewBold
                android:id="@+id/qn1"
                android:textColor="@color/claim_text_grey"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text Format('{0:MMM dd, yyy}',Payment.DepositDate)"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            local:MvxBind="Visible Payment.IsPaymentDateVisible"
            android:layout_marginBottom="8dp">
            <gsc.NunitoTextView
                android:textColor="@color/claim_text"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text PaymentDateLabel"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
            <gsc.NunitoTextViewBold
                android:id="@+id/qn1"
                android:textColor="@color/claim_text_grey"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text Format('{0:MMM dd, yyy}',Payment.PaymentDate)"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            local:MvxBind="Visible Payment.IsCashedDateVisible"
            android:layout_marginBottom="8dp">
            <gsc.NunitoTextView
                android:textColor="@color/claim_text"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text CashedDateLabel"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
            <gsc.NunitoTextViewBold
                android:id="@+id/qn1"
                android:textColor="@color/claim_text_grey"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text Format('{0:MMM dd, yyy}',Payment.CashedDate)"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            local:MvxBind="Visible Payment.IsStatusVisible"
            android:layout_marginBottom="8dp">
            <gsc.NunitoTextView
                android:textColor="@color/claim_text"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text ChequeStatusLabel"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
            <gsc.NunitoTextViewBold
                android:id="@+id/qn1"
                android:textColor="@color/claim_text_grey"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text Payment.Status"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp">
            <gsc.NunitoTextView
                android:textColor="@color/claim_text"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_marginTop="@dimen/claim_review_zero"
                local:MvxBind="Text  DateOfInquiryLabel"
                android:lineSpacingMultiplier="1.4"
                android:layout_gravity="left" />
            <gsc.NunitoTextViewBold
                android:layout_height="wrap_content"
                android:textColor="@color/claim_text_grey"
                android:textSize="13sp"
                android:layout_weight="1.0"
                android:layout_width="@dimen/claim_review_zero"
                android:gravity="left"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="@dimen/claim_review_zero"
                android:lineSpacingMultiplier="1.4"
                local:MvxBind="Text DateOfInquiry"
                android:layout_gravity="left" />
        </LinearLayout>
    </LinearLayout>
</ScrollView>
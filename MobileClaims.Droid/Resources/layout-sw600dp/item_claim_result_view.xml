                android:layout_weight="0.4"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="20dp"
                android:layout_centerVertical="true"
                android:layout_gravity="left"
                android:gravity="left"
                local:MvxBind="Text ClaimStatus;" />
        </TableRow>

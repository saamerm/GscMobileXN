<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:local="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">      
        <LinearLayout
            android:orientation="vertical"
            android:gravity="center"
            android:layout_weight="0.25"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/bnb_dashboard"
                android:background="@null"
                android:scaleType="centerInside"
                local:MvxBind="Click ShowDashboardCommand"/>
            <gsc.LeagueTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
				android:text="test text"
                android:textSize="@dimen/bnb_textsize"
                android:textColor="@color/white"
                android:singleLine="true"
                android:textAllCaps="true"
                local:MvxBind="Click ShowDashboardCommand; Text Dashboard" />
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:gravity="center"
            android:layout_weight="0.25"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/bnb_claims"
                android:background="@null"
				android:scaleType="centerCrop" 
                local:MvxBind="Click ShowClaimsCommand"/>
            <gsc.LeagueTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/bnb_textsize"
                android:textColor="@color/white"
                android:singleLine="true"
                android:textAllCaps="true"
                local:MvxBind="Click ShowClaimsCommand; Text Claims" />
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:gravity="center"
            android:layout_weight="0.25"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/bnb_findprovider"
                android:background="@null"
                android:scaleType="centerInside" 
                local:MvxBind="Click ShowFindAProviderCommand"/>
            <gsc.LeagueTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/bnb_textsize"
                android:textColor="@color/white"
                android:singleLine="true"
                android:textAllCaps="true"
                local:MvxBind="Click ShowFindAProviderCommand; Text FindProvider" />
        </LinearLayout>
		<LinearLayout
            android:gravity="center"
            android:layout_weight="0.25"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/bnb_hamburger"
                local:MvxBind="Click ShowLandingPageCommand"
                android:background="@null"
				android:scaleType="centerCrop"/>
            <gsc.LeagueTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/bnb_textsize"
                android:textColor="@color/white"
                android:textAllCaps="true"
                local:MvxBind="Click ShowLandingPageCommand; Text Menu" />
        </LinearLayout>
    </LinearLayout>
</FrameLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginTop="25dp"
                android:weightSum="1"
                local:MvxBind="Visibility Visibility(IsParticipantsListVisible);">
                <gsc.NunitoTextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/claimOtherGscPlanParticipantListLabel"
                    android:textColor="@color/red"
                    android:textSize="18sp"
                    android:layout_weight="0.5"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="15dp"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center" />
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="20dp"
                    android:layout_centerVertical="true"
                    android:gravity="center"
                    android:layout_weight="0.1"
                    android:layout_gravity="center"
                    android:layout_marginRight="5dp">
                    <ImageButton
                        android:id="@+id/errorButtonParticipantList"
                        android:layout_width="wrap_content"
                        android:layout_height="20dp"
                        android:background="@null"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:src="@drawable/claims_error_icon"
                        android:scaleType="centerInside"
                        android:onClick="errorClickHandler"
                        android:tag="@string/claimOtherGscPlanParticipantListError" />
                </LinearLayout>
                <Mvx.MvxSpinner
                    android:id="@+id/planParticipant"
                    android:layout_width="0dp"
                    android:textSize="18sp"
                    android:layout_weight="0.4"
                    android:prompt="@string/claimOtherGscPlanParticipantListLabel"
                    android:layout_height="wrap_content"
                    local:MvxBind="ItemsSource Participants; SelectedItem SelectedParticipant;"
                    local:MvxItemTemplate="@layout/item_othergsc_participants_spinner"
                    local:MvxDropDownItemTemplate="@layout/item_othergsc_participants_spinner_dropdown"
                    android:gravity="right"
                    android:layout_marginRight="20dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:spinnerMode="dialog" />
            </LinearLayout>
            <gsc.NunitoTextView
                android:id="@+id/claim_term_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:gravity="left"
                android:textSize="13sp"
                android:textAllCaps="false"

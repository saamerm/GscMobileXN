        <TableRow
            android:layout_marginTop="2dp"
            android:layout_marginBottom="5dp">
            <gsc.NunitoTextView
                android:text="@string/claimResultPaidAmountLabelPhone"
                android:textColor="@color/black"
                android:textSize="13sp"
                android:layout_marginLeft="20dp"
